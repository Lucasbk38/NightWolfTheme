{
  "name": "night-wolf",
  "displayName": "Night Wolf",
  "description": "A dark theme based on Night Owl",
  "version": "1.4.0",
  "publisher": "MaoSantaella",
  "license": "SEE LICENSE IN THE LICENSE FILE",
  "repository": {
    "type": "git",
    "url": "https://github.com/maoma87/NightWolfTheme"
  },
  "bugs": {
    "url": "https://github.com/maoma87/NightWolfTheme/issues",
    "email": "mauriciosantaella@outlook.com"
  },
  "engines": {
    "vscode": "^1.32.x"
  },
  "contributes": {
    "themes": [
      {
        "label": "Night Wolf [dark blue]",
        "uiTheme": "vs-dark",
        "path": "./themes/night-wolf-blue.json"
      },
      {
        "label": "Night Wolf [dark blue] (No Italics)",
        "uiTheme": "vs-dark",
        "path": "./themes/night-wolf-blue-noitalics.json"
      },
      {
        "label": "Night Wolf [dark gray]",
        "uiTheme": "vs-dark",
        "path": "./themes/night-wolf-gray.json"
      },
      {
        "label": "Night Wolf [dark gray] (No Italics)",
        "uiTheme": "vs-dark",
        "path": "./themes/night-wolf-gray-noitalics.json"
      },
      {
        "label": "Night Wolf [black]",
        "uiTheme": "vs-dark",
        "path": "./themes/night-wolf-black.json"
      },
      {
        "label": "Night Wolf [black] (No Italics)",
        "uiTheme": "vs-dark",
        "path": "./themes/night-wolf-black-noitalics.json"
      }
    ]
  },
  "scripts": {
    "dev": "node theme-compiler dev",
    "prod": "node theme-compiler prod",
    "pub-major": "vsce publish major",
    "pub-minor": "vsce publish minor",
    "pub-patch": "vsce publish patch",
    "vscode:prepublish": "node theme-compiler prod"
  },
  "devDependencies": {
    "color": "^3.1.0"
  },
  "keywords": [
    "Theme",
    "Dark Theme",
    "Night wolf",
    "Night Theme"
  ],
  "categories": [
    "Themes"
  ],
  "galleryBanner": {
    "color": "#101E2C",
    "theme": "dark"
